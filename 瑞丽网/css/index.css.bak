/*头部样式*/
*{
	margin:0px;
	padding:0px;
	list-style-type:none;
	text-decoration:none;
	font-size:"微软雅黑";
}
.yahei{
	font-family:"微软雅黑";
}
.left{
	float:left;
}
.right{float:right;}
.red{color:#c6171e;}
.clr{clear:both;}
#top{
	width:100%;
	height:30px;
	background:#ee1c24;
}
#top .topCon{
	width:1180px;
	height:25px;
	margin:0px auto;
	text-align:right;
	padding-top:5px;
}
#top .topCon p.topLeft{float:left;}
#top .topCon p.topLeft a.txtLeft{font-family:"微软雅黑";font-size:12px;color:#ffb4b6;}
#top .topCon p.topLeft a.txtLeft:hover{color:#fff;}
#top .topCon .search{
	/*width:230px;*/
	height:30px;
	float:right;
	position:relative;
	line-height:30px;
	/*display:none;*/
	overflow:hidden;
}
#top .topCon a.txtRight{
	width:25px;
	float:right;
	font-size:12px;
	text-decoration:none;
	color:#ffb4b6;
	line-height:30px;
	text-align:center;
	padding-left:15px;
}
#top .topCon a.txtRight:hover{
	color:#fff;
}
#top .topCon .seaImg{
	float:right;
	padding-top:6px;
}
#top .topCon .search input.seaTxt{
	width:230px;
	height:20px;
	background:#fff;
	border:0px;
	border-radius:5px;
	position:absolute;
	left:0px;
	top:5px;
	text-indent:30px;
	line-height:20px;
}
#top .topCon .search input.seaBtn{
	width:30px;
	height:20px;
	background:url("../images/search2.gif") no-repeat center;
	border:0px;
	position:absolute;
	left:0px;
	top:5px;
	cursor:pointer;
}
/*banner样式*/
.banner{width:100%;height:100px;background:#ee1c24;text-align:center;padding-top:30px;}
/*导航样式*/
#logo{width:100%;height:70px;background:#c6171e;}
#logo #logoCon{width:1180px;height:70px;margin:0px auto;}
#logo #logoCon dl{width:260px;height:60px;padding-top:10px;float:left;padding-right:20px;}
#logo #logoCon dl dt{width:50px;height:50px;float:left;font-size:22px;color:#fff;font-family:"微软雅黑";line-height:50px;}
#logo #logoCon dl dd{width:200px;height:50px;float:right;line-height:25px;}
#logo #logoCon dl dd a{color:#fff;text-decoration:none;font-family:"微软雅黑";font-size:16px;padding:0px 14px;}
#logo #logoCon dl dd a:hover{color:#e989ac;}
#logo #logoCon dl.menu2{width:200px;}
#logo #logoCon dl.menu2 dd{width:140px;}
#logo #logoCon dl.menu3{width:230px;}
#logo #logoCon dl.menu3 dd{width:170px;}
#logo #logoCon dl.menu3 dd a{position:relative;}
#logo #logoCon dl.menu3 dd a img.new{position:absolute;left:80px;top:5px;}
#logo #logoCon a.apps{width:150px;height:70px;display:block;background:green;float:left;background:url("../images/app.gif") no-repeat 30px 30px;}
#logo #logoCon a.apps:hover{background-position:30px 25px;}

/*头部动画样式*/
.flash{width:100%;height:500px;margin:0px auto;overflow:hidden;}
.flash .scroll{width:750px;height:500px;margin:0px auto;position:relative;}
.flash .scroll .prev{width:500px;height:500px;position:absolute;left:-500px;top:0px;background:#000;opacity:0.7;z-index:50;}
.flash .scroll .prev img{position:absolute;top:218px;right:20px;cursor:pointer;}
.flash .scroll .next{width:500px;height:500px;position:absolute;right:-500px;top:0px;background:#000;opacity:0.7;z-index:50;}
.flash .scroll .next img{position:absolute;top:218px;left:20px;cursor:pointer;}
.flash .scroll .imgList{width:2000%;height:500px;position:absolute;left:-750px;}
.flash .scroll .imgList a{float:left;}
.flash .scroll .scrollBtn{width:200px;position:absolute;bottom:20px;right:5%;}
.flash .scroll .scrollBtn ul li{width:5px;height:5px;border-radius:50%;background:#A5A5A5;margin:8px;float:left;}
.flash .scroll .scrollBtn ul li.hover{background:#e4393c;}
