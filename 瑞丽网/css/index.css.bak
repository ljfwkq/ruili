#footer div.footer_b div.footer_b_l div.contactUs ul li.first a.xinlang{background-position:0px 0px;}
#footer div.footer_b div.footer_b_l div.contactUs ul li.first a.xinlang:hover{background-position:0px -112px;}
#footer div.footer_b div.footer_b_l div.contactUs ul li.first a.weibo{background-position:-56px 0px;}
#footer div.footer_b div.footer_b_l div.contactUs ul li.first a.weibo:hover{background-position:-56px -112px;}
#footer div.footer_b div.footer_b_l div.contactUs ul li.first a.qZone{background-position:0px -56px;}
#footer div.footer_b div.footer_b_l div.contactUs ul li.first a.qZone:hover{background-position:0px -168px;}
#footer div.footer_b div.footer_b_l div.contactUs ul li.first a.renren{background-position:-56px -56px;}
#footer div.footer_b div.footer_b_l div.contactUs ul li.first a.renren:hover{background-position:-56px -168px;}

/*footer_b_m样式*/
#footer div.footer_b div.footer_b_m{width:580px;height:155px;border-left:1px solid #161616;border-right:1px solid #161616;float:left;}
#footer div.footer_b div.footer_b_m p.linkList{width:100%;height:14px;padding-top:20px;text-align:center;font-size:14px;color:#373737;margin-bottom:15px;}
#footer div.footer_b div.footer_b_m p.linkList a{font-size:14px;color:#373737;}
#footer div.footer_b div.footer_b_m p.linkList a:hover{color:#C6171E;}
#footer div.footer_b div.footer_b_m p.linkList span.bar{margin:0px 6px;}
#footer div.footer_b div.footer_b_m div.xkz_left{width:450px;line-height:24px;font-size:12px;color:#373737;text-align:center;float:left;}
#footer div.footer_b div.footer_b_m div.xkz_left a{font-size:12px;color:#373737;text-decoration:underline;}
#footer div.footer_b div.footer_b_m div.xkz_left a:hover{color:#C6171E;}
#footer div.footer_b div.footer_b_m div.xkz_right{width:120px;float:left;text-align:center;}
#footer div.footer_b div.footer_b_m div.xkz_right p{width:120px;line-height:24px;font-size:14px;color:#373737;}

/*footer_b_r样式*/
#footer div.footer_b div.footer_b_r{float:right;padding:18px 20px 0px 0px;} 
#footer div.footer_b div.footer_b_r p{height:16px;line-height:16px;font-family:"微软雅黑";font-size:16px;color:#373737;margin-bottom:30px;}
#footer div.footer_b div.footer_b_r p a{margin-left:12px;font-family:"微软雅黑";font-size:12px;color:#373737;}
#footer div.footer_b div.footer_b_r p a:hover{color:#C6171E;}
